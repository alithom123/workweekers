{
"manifest_version": 2,
    "name": "WorkWeekers",
    "short_name": "WorkWeekers",
    "description": "The 9-5 making it hard for you to get it up? I mean your smile! Every new tab's got a funny mantra to get you through the work-week.",
    "version": "0.0.0.5",
    "chrome_url_overrides" : {
        "newtab": "index.html"
    },
    "browser_action": {
        "default_title": "WorkWeekers",
        "default_icon": "images/icon_16x16.png"
    },
    "permissions": [
      "tabs",
      "storage"
    ],
    "icons": {
      "16": "images/icon_16x16.png",
      "48": "images/icon_48x48.png",
      "128": "images/icon_128x128.png"
        },
    "background": {
      "scripts": ["libraries/jquery-3.2.1.min.js", "scripts/background.js"],
      "persistent": false
  },
    "content_scripts": [{
      "matches": ["http://*/*"],
      "js": ["libraries/jquery-3.2.1.min.js", "scripts/content_workweekers.js"]
      }]
}
